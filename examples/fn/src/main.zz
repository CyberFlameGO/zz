using <stdio.h>::{printf};


// abstract
fntype AdderFn(int a, int b) -> int;
struct Math  {
    AdderFn add;
}

// impl
fn add_impl(int a, int b) -> int {
    return a+b;
}
fn new(AdderFn add) -> Math {
    return Math {
        .add = add,
    };
}

export fn main() -> int {
    Math m = new(add_impl);

    int x = m.add(1,8);
    printf("hello %u\n", x);
    return 0;
}
