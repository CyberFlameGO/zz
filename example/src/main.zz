import identity::some_helper;
import errors::error;

thread_local mutable float wee = 0.1;

atomic mutable int bob = 932;

fn gimme_wee() -> float {
    bob++;
    return wee;
}

export fn main() -> int {
    error err = {0};
    uint32_t bob;

    wee += 0.1;

    return some_helper(&err, &bob);
}
