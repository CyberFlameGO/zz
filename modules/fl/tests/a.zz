using fl;
using libc::stdio;

export fn main() -> int {
    fl::MAKE(b, 8, 1000);

    for (int i = 0; i < 10; i++) {
        printf("alloc %d\n", i);
        fl::alloc(b);
    }

    void* m1 = fl::alloc(b);
    void* m2 = fl::alloc(b);
    fl::release(b, m1);
    fl::release(b, m2);
    void* m3 = fl::alloc(b);
    fl::release(b, m3);

    return 0;
}
